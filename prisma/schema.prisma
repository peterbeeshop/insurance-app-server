generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model User {
  id                       String        @id @default(uuid()) @db.Uuid
  email                    String        @unique
  first_name               String 
  last_name                String
  password                 String
}